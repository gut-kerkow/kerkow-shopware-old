<!-- block name has to be exactly this to override a block in the rule component -->
{% block sw_condition_value_content %}
    <div class="sw-condition-currency sw-condition__condition-value">
        {% block idowapro_feed_generator_condition_product_operator %}
            <sw-condition-operator-select v-bind="{ operators, condition }"></sw-condition-operator-select>
        {% endblock %}

        {% block idowapro_feed_generator_condition_product_multi_select %}
            <sw-entity-multi-select
                size="medium"
                entity="value"
                :entityCollection="values"
                @change="setValueIds">
            </sw-entity-multi-select>
        {% endblock %}
    </div>
{% endblock %}
