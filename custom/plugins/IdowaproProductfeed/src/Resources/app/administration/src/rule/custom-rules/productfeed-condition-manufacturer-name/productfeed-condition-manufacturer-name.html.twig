<!-- block name has to be exactly this to override a block in the rule component -->
{% block sw_condition_value_content %}
    <div class="sw-condition-manufacturer-name sw-condition__condition-value">
        {% block idowapro_feed_generator_condition_manufacturer_name_operator %}
            <sw-condition-operator-select v-bind="{ operators, condition }"></sw-condition-operator-select>
        {% endblock %}

        {% block idowapro_feed_generator_condition_manufacturer_name_text_field %}
            <sw-text-field
                size="medium"
                v-model="value">
            </sw-text-field>
        {% endblock %}
    </div>
{% endblock %}
